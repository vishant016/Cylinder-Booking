            <fieldset>
                <input asp-for="Password" class="form-control" placeholder="Password....." />
                <span asp-validation-for="Password" class="text-danger"></span>
            </fieldset>
        </div>
        <div class="col-lg-4">
            <label asp-for="balance" class="control-label"><b>Balance:</b></label>
            <fieldset>
                <input asp-for="balance" class="form-control" placeholder="Balance....." />
                <span asp-validation-for="balance" class="text-danger"></span>
            </fieldset>
        </div>
        <div class="col-lg-4">
            <label asp-for="Bank" class="control-label"><b>Bank:</b></label>
            <fieldset>
                <input asp-for="Bank" class="form-control" placeholder="Bank....." />
                <span asp-validation-for="Bank" class="text-danger"></span>
            </fieldset>
        </div>
        <div class="col-lg-12">
            <fieldset>
                <button type="submit" class="main-button">Create</button>&nbsp;&nbsp;
                <a asp-action="Index" class="main-button">Cancle</a>
            </fieldset>
        </div>
    </form>
    <br /><br />
</div>
